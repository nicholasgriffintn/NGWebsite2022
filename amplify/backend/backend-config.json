{
  "storage": {
    "ngwebdb": {
      "service": "DynamoDB",
      "providerPlugin": "awscloudformation"
    },
    "ngwebassets": {
      "service": "S3",
      "providerPlugin": "awscloudformation",
      "dependsOn": []
    }
  },
  "auth": {
    "userPoolGroups": {
      "service": "Cognito-UserPool-Groups",
      "providerPlugin": "awscloudformation",
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "ngwebauth",
          "attributes": [
            "UserPoolId",
            "AppClientIDWeb",
            "AppClientID",
            "IdentityPoolId"
          ]
        }
      ]
    },
    "ngwebauth": {
      "service": "Cognito",
      "providerPlugin": "awscloudformation",
      "dependsOn": [],
      "customAuth": false,
      "frontendAuthConfig": {
        "socialProviders": [],
        "usernameAttributes": [
          "EMAIL"
        ],
        "signupAttributes": [
          "EMAIL"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyMinLength": "8",
          "passwordPolicyCharacters": [
            "REQUIRES_LOWERCASE",
            "REQUIRES_UPPERCASE",
            "REQUIRES_NUMBERS",
            "REQUIRES_SYMBOLS"
          ]
        },
        "mfaConfiguration": "OPTIONAL",
        "mfaTypes": [
          "TOTP"
        ],
        "verificationMechanisms": [
          "EMAIL"
        ]
      }
    }
  },
  "function": {
    "AdminQueriescdd05a74": {
      "service": "Lambda",
      "providerPlugin": "awscloudformation",
      "build": true,
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "ngwebauth",
          "attributes": [
            "UserPoolId"
          ]
        }
      ]
    }
  },
  "api": {
    "AdminQueries": {
      "service": "API Gateway",
      "providerPlugin": "awscloudformation",
      "authorizationType": "AMAZON_COGNITO_USER_POOLS",
      "dependsOn": [
        {
          "category": "auth",
          "resourceName": "ngwebauth",
          "attributes": [
            "UserPoolId"
          ]
        },
        {
          "category": "function",
          "resourceName": "AdminQueriescdd05a74",
          "attributes": [
            "Arn",
            "Name"
          ]
        }
      ]
    }
  }
